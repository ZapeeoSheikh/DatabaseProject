@using MyWebProject.Models
@model List<Student>


<h2>Student Details </h2>


<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th> ID </th>

            <th> Name </th>

            <th> Email </th>

            <th> Phone </th>

            <th> Actions </th>
        </tr>
    </thead>

    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.Email
                </td>
                <td>
                    @item.Phone
                </td>
                <td>
                    <a href="/Home/SView/@item.Id"><span class="glyphicon glyphicon-eye-open"></span></a> |
                    <a href="/Home/SEdit/@item.Id"><span class="glyphicon glyphicon-edit"></span></a> |
                    <a href="/Home/SDelete/@item.Id"><span class="glyphicon glyphicon-trash"></span></a>
                </td>

            </tr>
        </tbody>
    }
</table>


