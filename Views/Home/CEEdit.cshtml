@using MyWebProject.Models
@model CnicDetails
@{
    ViewBag.Title = "CEEdit";
}


<div class="container col-lg-3 col-lg-push-3">
    <div class="panel  panel-primary ">
        <div class="panel-heading">
            Edit Cnic Details @Model.Id
        </div>
        <div class="panel-body ">
            <form action="/Home/CEEdit" method="post">
                <div class="form-group">
                    <label for="semail"> Student Id :</label>
                    <input type="text" class="form-control" id="semail" name="StudentId" value="@Model.StudentId">
                </div>
                <div class="form-group">
                    <label for="email"> Cnic Number :</label>
                    <input type="text" class="form-control" id="email" name="CnicNumber" value="@Model.CnicNumber">
                </div>

                <button type="submit" class="btn btn-primary">Add Now</button>
            </form>
        </div>
    </div>
</div>