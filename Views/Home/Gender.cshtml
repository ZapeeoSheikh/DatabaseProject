@using MyWebProject.Models
@using System.Configuration
@model List<Gender>



<table class="table table-striped table-bordered col-lg-push-6">
    <thead>
        <tr>
            <td>
                ID
            </td>

            <td>
                Name
            </td>
            <td>
                Gender
            </td>

        </tr>
    </thead>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.Name
            </td>
            <td>
                @item.Genderr
            </td>

        </tr>
    }




</table>