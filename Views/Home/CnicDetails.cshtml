@using MyWebProject.Models

@model List<CnicDetails>

<h2>Cnic Details </h2>

<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>
                ID
            </th>

            <th>
                Student ID
            </th>

            <th>
                Cnic Number
            </th>
            <th>
                Actions
            </th>


        </tr>
    </thead>

    @foreach (var item in Model)
    {
        <tbody>
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.StudentId
                </td>
                <td>
                    @item.CnicNumber
                </td>
                <td>
                    <a href="/Home/CDView/@item.Id"><span class="glyphicon glyphicon-eye-open"></span></a> |
                    <a href="/Home/CEEdit/@item.Id"><span class="glyphicon glyphicon-edit"></span></a> |
                    <a href="/Home/CDDelete/@item.Id"><span class="glyphicon glyphicon-trash"></span></a>
                </td>
            </tr>
        </tbody>
    }
</table>
