@using MyWebProject.Models

@model List<CnicDetails>

<h2>Cnic Details </h2>

<table class="table table-striped">
    <tr>
        <td>
            ID
        </td>

        <td>
            Student ID
        </td>

        <td>
            Cnic Number
        </td>


    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.StudentId
            </td>
            <td>
                @item.CnicNumber
            </td>

        </tr>
    }
</table>