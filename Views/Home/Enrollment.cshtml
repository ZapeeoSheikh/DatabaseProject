@using MyWebProject.Models

@model List<Enrollment>

<h2>Enrollments Details </h2>

<table class="table table-striped table-bordered col-lg-push-6">
    <tr>
        <td>
            ID
        </td>

        <td>
            Student ID
        </td>

        <td>
            Course ID
        </td>

        <td>
            Joining Date
        </td>


    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Id
            </td>
            <td>
                @item.StudentId
            </td>
            <td>
                @item.CourseId
            </td>
            <td>
                @item.JoinOndate
            </td>

        </tr>
    }
</table>