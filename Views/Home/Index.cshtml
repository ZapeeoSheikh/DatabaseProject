
@{
    ViewBag.Title = "Index";
}

<h2>Index File </h2>



<div class="container col-lg-4 col-lg-push-2 col-lg-pull-3 ">
    <h2>Update DataBase Here</h2>
    <p>Click on the collapsible panel to open and close it.</p>
    <div class="panel-group">
        <div class="panel panel-success">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse2">Update Student Table</a>
                </h4>
            </div>
            <div id="collapse2" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-success ">
                        <div class="panel-heading">
                            Student Database
                        </div>
                        <div class="panel-body panel-success">
                            <form action="/Home/Students" method="post">
                                <div class="form-group">
                                    <label for="email"> Name :</label>
                                    <input type="text" class="form-control" id="email" name="Name">
                                </div>
                                <div class="form-group">
                                    <label for="email">Email :</label>
                                    <input type="email" class="form-control" id="email" name="Email">
                                </div>
                                <div class="form-group">
                                    <label for="phone">Phone :</label>
                                    <input type="text" class="form-control" id="phone" name="Phone">
                                </div>
                                <div class="form-group">
                                    <label for="pwd">Password :</label>
                                    <input type="password" class="form-control" id="pwd" name="Password">
                                </div>
                                <button type="submit" class="btn btn-success">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-danger">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse1">Update Gender Table</a>
                </h4>
            </div>
            <div id="collapse1" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-danger ">
                        <div class="panel-heading">
                            Gender DataBase
                        </div>
                        <div class="panel-body panel-danger">
                            <form action="/Home/Gender" method="post">
                                <div class="form-group">
                                    <label for="email"> Name :</label>
                                    <input type="text" class="form-control" id="email" name="Name">
                                </div>
                                <div class="form-group">
                                    <label for="pwd">Gender :</label>
                                    <input type="text" class="form-control" id="pwd" name="Genderr">
                                </div>
                                <button type="submit" class="btn btn-danger">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-info">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse3">Update Enrollment Table</a>
                </h4>
            </div>
            <div id="collapse3" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-primary ">
                        <div class="panel-heading">
                            Enrollment DataBase
                        </div>
                        <div class="panel-body panel-primary">
                            <form action="/Home/Enrollment" method="post">
                                <div class="form-group">
                                    <label for="email"> Student ID :</label>
                                    <input type="text" class="form-control" id="email" name="StudentId">
                                </div>
                                <div class="form-group">
                                    <label for="courseid">Course ID :</label>
                                    <input type="text" class="form-control" id="courseid" name="CourseId">
                                </div>
                                <div class="form-group">
                                    <label for="join">Joining Date :</label>
                                    <input type="text" class="form-control" id="join" name="JoinOndate">
                                </div>
                                <button type="submit" class="btn btn-primary">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-warning">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse4">Update Course Table</a>
                </h4>
            </div>
            <div id="collapse4" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-warning ">
                        <div class="panel-heading">
                            Course DataBase
                        </div>
                        <div class="panel-body panel-warning">
                            <form action="/Home/Course" method="post">
                                <div class="form-group">
                                    <label for="email"> Title :</label>
                                    <input type="text" class="form-control" id="email" name="Title">
                                </div>
                                <div class="form-group">
                                    <label for="cost">Cost :</label>
                                    <input type="text" class="form-control" id="cost" name="Cost">
                                </div>
                                <div class="form-group">
                                    <label for="des">Description :</label>
                                    <input type="text" class="form-control" id="des" name="Description">
                                </div>
                                <button type="submit" class="btn btn-warning">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <h4 class="panel-title">
                    <a data-toggle="collapse" href="#collapse5">Update Cnic Detail Table</a>
                </h4>
            </div>
            <div id="collapse5" class="panel-collapse collapse">



                <div class="container col-lg-10 col-lg-push-1">
                    <div class="panel  panel-default ">
                        <div class="panel-heading">
                            Cnic Details DataBase
                        </div>
                        <div class="panel-body panel-default">
                            <form action="/Home/CnicDetails" method="post">
                                <div class="form-group">
                                    <label for="cnic"> CNIC Number :</label>
                                    <input type="text" class="form-control" id="cnic" name="CnicNumber">
                                </div>
                                <div class="form-group">
                                    <label for="pwd">Student ID :</label>
                                    <input type="text" class="form-control" id="pwd" name="StudentId">
                                </div>
                                <button type="submit" class="btn btn-default">Add Now</button>
                            </form>
                        </div>
                    </div>
                </div>





            </div>
        </div>
    </div>
</div>
